<a class="comm_h" href="/">Home</a>
<a class="comm_s" href="/session">Personal Collection</a>

<p class="comm_m">Community Collection</p>

<table class="comm_t">
    <thead>
     <tr>
      
      <th scope="col">Username</th>
      <th scope="col">Title</th>
      <th scope="col">Year</th>
      <th scope="col">Notes</th>
      <th scope="col">Rating(1-5)</th>
      <th scope="col">Category</th>
     
      
    </tr>
    </thead>

<tbody>
    <% games.each do |game| %>
    <tr>
      
      <td><%= game["user_name"] %></td>
      <td><%= game["title"] %></td>
      <td><%= game["year"] %></td>
      <td><%= game["description"] %></td>
      <td><%= game["rating"] %></td>
      <td><%= game["category"] %></td>
      
      
    </tr>
    <% end %>
  </tbody>
</table>
